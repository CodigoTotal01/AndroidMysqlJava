<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".features.store.pages.GeneralActivity">

    <Button
        android:id="@+id/btnCatalogoProductos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Catalogo de productos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCatalogoVentaProductos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Catalogo Venta de Productos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCatalogoProductos" />

    <Button
        android:id="@+id/btnReporteVentas"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Reporte de Ventas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCatalogoVentaProductos" />

    <Button
        android:id="@+id/btnReporteClientes"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Reporte de clientes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnReporteVentas" />

    <Button
        android:id="@+id/btnReporteEmpleados"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="reporte de empleados"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnReporteClientes" />

</androidx.constraintlayout.widget.ConstraintLayout>